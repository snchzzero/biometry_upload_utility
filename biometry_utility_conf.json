{
  "source_biometry_folder": "/home/evstafev/Изображения/biorecord",
  "protocol": "http",
  "domain": "third-home",
  "logging": {
        "version": 1,
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "formatter": "default",
                "level": "CRITICAL"
            },
            "file": {
                "class": "logging.handlers.RotatingFileHandler",
                "filename": "loggers/biometry-utility.log",
                "maxBytes": 1048576,
                "backupCount": 4,
                "formatter": "default",
                "level": "DEBUG"
            }
        },
        "loggers":{
            "biometry_utility": {
                "handlers": ["file"],
                "level": "DEBUG"
            }
        },
        "formatters": {
            "default": {
                "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
            }
        },
        "root": {
            "level": "ERROR",
            "handlers": ["console"]
        }
    }
}