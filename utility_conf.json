{
  "utility_settings":
    {
      "source_biometry_folder": "biorecord_photos",
      "biometry_type_id": "95b312bf0be942b19d69fdcb7cdfd6c6",
      "biometry_upload_url": "{}://biometry.{}/upload"
    },
  "logging": {
        "version": 1,
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "formatter": "default",
                "level": "CRITICAL"
            },
            "file": {
                "class": "logging.handlers.RotatingFileHandler",
                "filename": "loggers/biometry-utility.log",
                "maxBytes": 1048576,
                "backupCount": 4,
                "formatter": "default",
                "level": "DEBUG"
            }
        },
        "loggers":{
            "biometry_utility": {
                "handlers": ["file"],
                "level": "DEBUG"
            },
          "endpoint_service": {
                "handlers": ["file"],
                "level": "DEBUG"
          }
        },
        "formatters": {
            "default": {
                "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
            }
        },
        "root": {
            "level": "ERROR",
            "handlers": ["console"]
        }
    }
}
